<link rel="stylesheet" href="/CSS/chat.css">
</head>

<body>
   <div class="wrapper">
      <div class="one">
         <div class="self">
            <div class="self__user-img">
               <img
                  src="https://d5nunyagcicgy.cloudfront.net/external_assets/hero_examples/hair_beach_v391182663/original.jpeg"
                  alt="<%= user.name %> image">
               <div class="online-state">
                  <div class="state-text">Online</div>
               </div>
            </div>
            <div class="self__user-name">
               <%= user.name %>
            </div>
            <div class="self__user-email">
               <%= user.email %>
            </div>
         </div>
      </div>

      <div class="add-user">
         <form action="/user?_method=patch" method="POST">
            <input type="text" name="email" placeholder="Add contact by email">
            <button>ADD</button>
         </form>
      </div>

      <div class="two">
         <a href="/user/sign-out">
            <img id="sign-out"
               src="data:image/svg+xml;base64," />
         </a>
      </div>

      <div class="three">
         <input type="text" placeholder="Search">
         <img class="input-icon"
            src="data:image/svg+xml;base64," />
      </div>

      <div class="four">
         <% contacts.forEach(contact=> {%>
            <a href="/user/<%= contact._id %>">
               <div class="user-container">
                  <dir class="user-img">
                     <img src="https://miro.medium.com/max/1200/1*mk1-6aYaf_Bes1E3Imhc0A.jpeg"
                        alt="<%= contact.name %> image">
                     <div class="online-state"></div>
                  </dir>
                  <div class="user-wrapper">
                     <h4 class="user-name">
                        <%= contact.name %>
                     </h4>
                     <div class="unread-messages"><span>3</span></div>
                     <h5 class="last-message">Hi Danna how are you Do...</h5>
                  </div>
               </div>
            </a>
            <% }) %>
      </div>

      <div class="five">
         <img class="user-image" src="https://miro.medium.com/max/1200/1*mk1-6aYaf_Bes1E3Imhc0A.jpeg"
            alt="<%=ConversationContact.name %> image">
         <span class="user-name">
            <%=ConversationContact.name %>
         </span>
         <span class="online-state"></span>
         <span class="user-email">
            <%= ConversationContact.email %>
         </span>
      </div>

      <div class="six">Six</div>

      <div class="seven">
         <form action="">
            <input type="text" placeholder="Type something to send...">
            <button>SEND</button>
         </form>
      </div>
   </div>